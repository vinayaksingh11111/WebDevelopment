<%- include("partials/header") %>
<h1>Home</h1>
<p><%= homeContent %> </p>
<% posts.forEach(function(post){ %>
<h1><%= post.Title %> </h1>
<div class="postBody">
    <p><%= post.Body.substring(0,101)+"..." %> </p>
    <a href="http://localhost:3000/posts/<%= post.Title %>) %>" >Read More</a>
</div>
<% }) %>
<%- include("partials/footer") %>